{
  "name": "ag-table",
  "version": "1.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4200",
    "build": "ng build --aot --configuration production",
    "deploy-git-pages": "ng build --aot --configuration production --base-href=/ag-table/ && node ./node_modules/gh-pages/bin/gh-pages -d dist",
    "build-publish": "npm run build-pkg && npm run publish-pkg",
    "build-pkg": "rimraf dist && npm run packagr && node copy && cd dist && npm pack",
    "publish-pkg": "pushd dist && npm publish",
    "packagr": "ng-packagr -p projects/lib/package.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.2.5",
    "@angular/cdk": "^13.2.5",
    "@angular/common": "~13.2.5",
    "@angular/compiler": "~13.2.5",
    "@angular/core": "~13.2.5",
    "@angular/forms": "~13.2.5",
    "@angular/material": "^13.2.5",
    "@angular/platform-browser": "~13.2.5",
    "@angular/platform-browser-dynamic": "~13.2.5",
    "@angular/router": "~13.2.5",
    "animate.css": "^3.7.2",
    "bootstrap": "^4.3.1",
    "chance": "^1.1.8",
    "core-js": "^2.5.4",
    "ngx-highlightjs": "^3.0.3",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.5",
    "@angular/cli": "~13.2.5",
    "@angular/compiler-cli": "~13.2.5",
    "@angular/language-service": "~13.2.5",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cpx": "^1.5.0",
    "gh-pages": "^2.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.5"
  }
}